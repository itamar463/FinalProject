<Window x:Class="FinalProject.Demos.StudentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProject.Demos"
        mc:Ignorable="d"
        Title="Student" Height="450" Width="800">
    <Grid Background="AliceBlue">
        <StackPanel Orientation="Horizontal">
            <Label Content="Enter exam name " HorizontalAlignment="Center" VerticalAlignment="Top" Margin="200,125,0,0" FontSize="15"/>
            <TextBox Name="StudentExamTXT" HorizontalAlignment="Left" Margin="10,125,0,0" VerticalAlignment="Top" Width="204" Height="27"/>
        </StackPanel>
        <Button Name="EnterExam" Content="Enter Exam" HorizontalAlignment="Left" Margin="400,0,0,0" VerticalAlignment="Center" Width="87" Click="EnterExam_Click" Height="30" />
        <Label Name="StudentLbl" Content="Welcome " HorizontalAlignment="Left" Margin="200,56,0,0" VerticalAlignment="Top" FontSize="25"/>
    </Grid>
</Window>
