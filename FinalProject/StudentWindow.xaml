        xmlns:local="clr-namespace:FinalProject"
        mc:Ignorable="d"
        Title="Student" Height="450" Width="800">
    <Grid Background="AliceBlue">
        <Label Name="StudentExamLBL" Content="Exam Name:" HorizontalAlignment="Left" Margin="284,125,0,279"/>
